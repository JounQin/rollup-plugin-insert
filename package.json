{
  "name": "rollup-plugin-insert",
  "version": "1.0.0",
  "description": "string mutation plugin for rollup",
  "main": "index.js",
  "module": "insert.js",
  "jsnext:main": "insert.js",
  "repository": "https://github.com/JounQin/rollup-plugin-insert.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "files": [
    "index.js",
    "insert.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --max-warnings 10",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "1stg"
  },
  "eslintIgnore": [
    "coverage",
    "node_modules",
    "index.js",
    "test",
    "!test/insert.spec.js"
  ],
  "jest": {
    "collectCoverageFrom": [
      "insert.js"
    ],
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "rollup": "*"
  },
  "dependencies": {
    "magic-string": "^0.25.2",
    "rollup-pluginutils": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^6.0.1",
    "eslint-config-1stg": "^4.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.2",
    "rollup-plugin-buble": "^0.19.6"
  }
}
