{
  "name": "rollup-plugin-insert",
  "version": "1.2.0",
  "description": "string mutation plugin for rollup",
  "repository": "https://github.com/JounQin/rollup-plugin-insert.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/cjs",
  "module": "lib",
  "es2015": "lib/es2015",
  "fesm5": "lib/esm",
  "types": "lib",
  "files": [
    "lib",
    "!lib/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -e named -p",
    "build:ts": "tsc -b",
    "clean": "rimraf lib",
    "lint": "eslint . --cache --ext .js,.ts --max-warnings 10 -f friendly",
    "prepublishOnly": "yarn test && yarn build",
    "pretest": "yarn clean",
    "test": "jest"
  },
  "peerDependencies": {
    "rollup": "*"
  },
  "dependencies": {
    "magic-string": "^0.25.6",
    "rollup-pluginutils": "^2.8.2"
  },
  "devDependencies": {
    "@1stg/lib-config": "^0.1.21",
    "@types/jest": "^25.1.0",
    "@types/node": "^13.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "jest": {
    "collectCoverage": true,
    "preset": "ts-jest"
  }
}
